  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@icon-park/react": "^1.4.2",
    "@mgdn/bvid": "^0.1.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "ahooks": "3.7.8",
    "antd": "^5.11.0",
    "axios": "0",
    "axios-userscript-adapter": "0",
    "classnames": "^2.3.2",

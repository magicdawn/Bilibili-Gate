    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@icon-park/react": "^1.4.2",
    "@mgdn/bvid": "^0.2.0",

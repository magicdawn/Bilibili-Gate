    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.8.1",
    "react-virtuoso": "^4.7.1",
    "ua-parser-js": "^1.0.37",
    "valtio": "^1.13.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"

{
  "name": "bilibili-app-recommend",
  "version": "0.18.2",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev:less": "tlm src --watch",
    "dev": "concurrently -k npm:dev:less vite",
    "typecheck": "tsc --noEmit",
    "build:less": "tlm src",
    "build:vite": "vite build",
    "//": "//",
    "build:real-build": "turbo build:less typecheck build:vite",
    "build": "pnpm build:real-build && pnpm preview",
    "build:vite-raw": "vite build -- --no-minify",
    "build:vite-analyze": "vite build -- --no-minify --analyze",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@emotion/babel-plugin": "^11.11.0",
    "@magicdawn/prettier-config": "^0.0.2",
